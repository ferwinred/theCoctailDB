{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/app",
  "projects": {
    "prueba_inlaze": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "authorization": {
      "type": "application",
      "root": "apps/authorization",
      "entryFile": "main",
      "sourceRoot": "apps/authorization/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authorization/tsconfig.app.json"
      }
    }
  }
}